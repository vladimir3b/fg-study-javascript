<pre class="line-numbers"><code class="language-js">const observable = Observable.create(observer => {
  let start = 1;
  setInterval(() => {
    observer.next(`hi ${start++}`);
    if (start >= 10) {
      observer.complete();
    }
    if (_.random(10, 50) % 2 === 0) {
      observer.error('There was an even random value.');
    }
  }, 1000);
});
observable.subscribe(
  console.log,
  console.warn,
  () => console.log('Observable completed.')
);
